<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Rock_Paper_Scissors.mento_menti_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo3_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mento_Menti_App">
        <activity android:name="com.Rock_Paper_Scissors.mento_menti_app.IntroActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Rock_Paper_Scissors.mento_menti_app.RunningActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.Rock_Paper_Scissors.mento_menti_app.MainActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.Rock_Paper_Scissors.mento_menti_app.HappyEndingAcitivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.Rock_Paper_Scissors.mento_menti_app.BadEndingActivity" android:screenOrientation="portrait"></activity>
    </application>
</manifest>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mento_menti_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mento_Menti_App">
        <activity android:name=".IntroActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RunningActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".MainActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".HappyEndingAcitivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".BadEndingActivity" android:screenOrientation="portrait"></activity>
    </application>
</manifest>